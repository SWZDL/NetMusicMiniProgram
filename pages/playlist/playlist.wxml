<view class="songs">
    <block wx:for="{{songs}}" wx:key="*this">
        <view class="song" bind:tap="goToSong" data-song-id="{{item.id}}">
            <text class="song-index">{{index}}</text>
            <text class="song-name">{{item.name}}</text>
            -
            <text class="song-singer">{{item.ar[0].name}}</text>
        </view>
    </block>
</view>