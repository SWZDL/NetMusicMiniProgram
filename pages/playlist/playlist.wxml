<l-card type="primary"
        image="{{playlist.coverImgUrl}}"
        title="{{playlist.name}}"
        l-img-class="playlist-card-image"
        l-title-class="playlist-card-title"
        l-class="playlist-card">
    <!-- 此处为content -->
    <view class="playlist-card-content">
        <view class="playlist-card-content-tag">
            <block wx:for="{{playlist.tags}}" wx:key="*this">
                <l-tag shape="circle"
                       bg-color="#f3f3f3"
                       font-color="#3963BC"
                       size="large">
                    {{item}}
                </l-tag>
            </block>
        </view>
        <text class="playlist-card-content-text">{{playlist.description}}</text>
    </view>
</l-card>

